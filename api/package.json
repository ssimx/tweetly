{
    "name": "server",
    "version": "1.0.0",
    "main": "dist/server.js",
    "type": "module",
    "scripts": {
        "dev": "dotenv -e .env tsx watch ./src/server.ts",
        "test": "dotenv -e .env ts-node ./src/server.ts",
        "copy-prisma-schema": "cp -r ./prisma ./dist/",
        "build": "npm install && tsc && prisma generate",
        "start": "node ./dist/server.js",
        "migrate-dev": "dotenv -e .env -- npx prisma migrate dev",
        "prisma-studio-dev": "dotenv -e .env -- npx prisma studio",
        "migrate-prod": "dotenv -e .env.production -- npx prisma migrate deploy"
    },
    "prisma": {
        "seed": "node prisma/seed.js"
    },
    "keywords": [],
    "author": "vespucci",
    "license": "ISC",
    "description": "",
    "devDependencies": {
        "@types/bcrypt": "^5.0.2",
        "@types/express": "^4.17.21",
        "@types/node": "^22.13.4",
        "@types/passport": "^1.0.16",
        "@types/passport-jwt": "^4.0.1",
        "nodemon": "^3.1.4",
        "tsx": "^4.19.3"
    },
    "dependencies": {
        "@faker-js/faker": "^9.1.0",
        "@prisma/client": "^6.6.0",
        "@types/multer": "^1.4.12",
        "bcrypt": "^5.1.1",
        "cloudinary": "^1.41.3",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.21.0",
        "form-data": "^4.0.2",
        "jsonwebtoken": "^9.0.2",
        "multer": "^1.4.5-lts.1",
        "multer-storage-cloudinary": "^4.0.0",
        "passport": "^0.7.0",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "prisma": "^6.6.0",
        "socket.io": "^4.8.0",
        "socketio-jwt-auth": "^0.2.1",
        "ts-node": "^10.9.2",
        "tweetly-shared": "^2.0.1",
        "typescript": "^5.8.3",
        "unique-username-generator": "^1.3.0",
        "zod": "^3.24.2"
    }
}
